{
	"name": "majsoul-api",
	"version": "1.0.0",
	"description": "",
	"typings": "dist/index.d.ts",
	"module": "dist/index.js",
	"scripts": {
		"dev": "tsc --watch",
		"connect": "node -r source-map-support/register --insecure-http-parser ./dist/connector.js",
		"rest": "node -r source-map-support/register ./dist/rest-api.js",
		"proto:fetch": "curl https://mahjongsoul.game.yo-star.com/v0.10.63.w/res/proto/liqi.json > src/majsoul/types/liqi.json",
		"proto:generate": "pbjs -t static --no-create --no-encode --no-decode --no-verify --no-delimited --no-beautify --no-convert --no-service src/majsoul/types/liqi.json | pbts --no-comments -o src/majsoul/types/liqi.d.ts -",
		"proto:admin:generate": "pbjs -t static --no-create --no-encode --no-decode --no-verify --no-delimited --no-beautify --no-convert --no-service src/majsoul/admin/types/proto.json | pbts --no-comments -o src/majsoul/admin/types/proto.d.ts -",
		"cert:create": "openssl req -x509 -newkey rsa:4096 -keyout dist/riichi.key.pem -out dist/riichi.crt.pem -sha256 -days 365 -nodes"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"express-jwt": "^6.0.0",
		"express-validator": "^6.14.2",
		"googleapis": "^51.0.0",
		"install": "^0.13.0",
		"jsonwebtoken": "^8.5.1",
		"mongodb": "^3.5.8",
		"node-fetch": "^2.6.1",
		"protobufjs": "^6.11.2",
		"rxjs": "^7.5.5",
		"syanten": "^1.5.0",
		"user-agents": "^1.0.1001",
		"uuid": "^3.3.3",
		"ws": "^7.1.2"
	},
	"devDependencies": {
		"@types/cors": "^2.8.6",
		"@types/express": "^4.17.6",
		"@types/express-jwt": "^0.0.42",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/mongodb": "^3.5.20",
		"@types/node-fetch": "^2.5.7",
		"@types/ws": "^7.2.4",
		"eslint": "^6.5.1",
		"source-map-support": "^0.5.19",
		"typescript": "^4.2.3"
	}
}
