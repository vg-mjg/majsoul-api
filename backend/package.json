{
	"packageManager": "yarn@3.2.3",
	"name": "backend",
	"main": "dist/index.js",
	"version": "1.0.0",
	"files": [
		"dist/*"
	],
	"scripts": {
		"dev": "tsc --watch",
		"connect": "node -r source-map-support/register ./dist/connector.js",
		"rest": "node -r source-map-support/register ./dist/rest-api.js",
		"cli": "node -r source-map-support/register ./dist/cli.js",
		"sss": "node -r source-map-support/register ./dist/sss.js",
		"cert:create": "openssl req -x509 -newkey rsa:4096 -keyout dist/riichi.key.pem -out dist/riichi.crt.pem -sha256 -days 365 -nodes"
	},
	"dependencies": {
		"@googleapis/sheets": "^3.0.2",
		"@types/cors": "^2.8.6",
		"@types/express": "^4.17.6",
		"@types/express-jwt": "^6.0.4",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/node-fetch": "^2.5.7",
		"@types/seedrandom": "^3.0.2",
		"cors": "^2.8.5",
		"express": "^4.18.1",
		"express-jwt": "^7.7.5",
		"express-validator": "^6.14.2",
		"jsonwebtoken": "^9.0.0",
		"majsoul": "^1.0.0",
		"mongodb": "^4.9.1",
		"node-fetch": "^2.6.7",
		"protobufjs": "^7.1.0",
		"rxjs": "^7.5.6",
		"seedrandom": "^3.0.5",
		"syanten": "^1.5.0",
		"typescript": "^4.8.2",
		"user-agents": "^1.0.1125",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@types/jest": "^29.0.0",
		"jest": "^29.0.1",
		"source-map-support": "^0.5.21",
		"ts-jest": "^28.0.8"
	},
	"sideEffects": false
}
